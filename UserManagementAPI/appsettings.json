{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "SecretKey": "your-secret-key-here",
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "ExpirationInMinutes": 60
  },
  "AppSettings": {
    "ApiName": "UserManagementAPI",
    "Version": "1.0.0",
    "EmailConfirmationRequired": true,
    "MaxLoginAttempts": 5
  },
  "AllowedHosts": "*"
}
